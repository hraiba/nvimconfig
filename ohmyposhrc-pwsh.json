{
  "$schema": "https://raw.githubusercontent.com/JanDeDobbeleer/oh-my-posh/main/themes/schema.json",
  "version": 2,
  "blocks": [
    {
      "type": "prompt",
      "alignment": "left",
      "segments": [
        {
          "type": "os",
          "style": "plain",
          "foreground": "#26C6DA",
          "template": "üòà ",
          "properties": {
            "macos": "mac"
          }
        },
        {
          "type": "path",
          "style": "plain",
          "foreground": "lightGreen",
          "template": "[{{ .Path }}] ",
          "properties": {
            "style": "folder"
          }
        },
        {
          "type": "git",
          "style": "powerline",
          "foreground": "#ff7b6c",
          "foreground_templates": [
            "{{ if or (.Working.Changed) (.Staging.Changed) }}#fa4204{{ end }}",
            "{{ if and (gt .Ahead 0) (gt .Behind 0) }}#FFCC80{{ end }}",
            "{{ if gt .Ahead 0 }}#B388FF{{ end }}",
            "{{ if gt .Behind 0 }}#B388FB{{ end }}",
            "{{ if gt or .UpstreamGone }}#c41906 {{ end }}"
          ],
          "template": "[{{ .HEAD }}{{if .BranchStatus }} {{ .BranchStatus }}{{ end }}{{ if .Working.Changed }} ÔÅÑ {{ .Working.String }}{{ end }}{{ if and (.Working.Changed) (.Staging.Changed) }} |{{ end }}{{ if .Staging.Changed }} ÔÅÜ {{ .Staging.String }}{{ end }}{{ if gt .StashCount 0 }} Ôöí {{ .StashCount }}{{ end }}] ",
          "properties": {
            "fetch_status": true,
            "fetch_stash_count": true,
            "fetch_upstream_icon": true,
            "untracked_modes": {
              "/Users/user/Projects/oh-my-posh/": "no"
            }
          }
        },
        {
          "type": "dotnet",
          "style": "plain",
          "foreground": "#906cff",
          "template": "[\ue70c {{ .Full }}] "
        },
        {
          "type": "python",
          "style": "plain",
          "foreground": "#906cff",
          "template": "[\ue235 {{ if .Error }}{{ .Error }}{{ else }}{{ if .Venv }}{{ .Venv }} {{ end }}{{ .Full }}{{ end }}] "
        },
        {
          "type": "go",
          "style": "plain",
          "foreground": "#7FD5EA",
          "template": "[\ufcd1 {{ if .Error }}{{ .Error }}{{ else }}{{ .Full }}{{ end }}] "
        },
        {
          "type": "node",
          "style": "plain",
          "foreground": "#76b367",
          "template": "[\ue718 {{ if .PackageManagerIcon }}{{ .PackageManagerIcon }} {{ end }}{{ .Full }}] "
        },
        {
          "type": "ruby",
          "style": "plain",
          "foreground": "#f44336",
          "template": "[\ue791{{ if .Error }}{{ .Error }}{{ else }}{{ .Full }}{{ end }}] "
        },
        {
          "type": "java",
          "style": "plain",
          "foreground": "#ea2d2e",
          "template": "[\ue738 {{ if .Error }}{{ .Error }}{{ else }}{{ .Full }}{{ end }}] "
        },
        {
          "type": "julia",
          "style": "plain",
          "foreground": "#4063D8",
          "template": " \ue624 {{ if .Error }}{{ .Error }}{{ else }}{{ .Full }}{{ end }} "
        },
        {
          "type": "text",
          "style": "plain",
          "foreground": "#FFD54F",
          "template": "‚ö° "
        }
      ]
    }
  ]
}
