{
  "$schema": "https://raw.githubusercontent.com/JanDeDobbeleer/oh-my-posh/main/themes/schema.json",
  "version": 2,
  "blocks": [
    {
      "type": "prompt",
      "alignment": "left",
      "segments": [
        {
          "type": "os",
          "style": "plain",
          "foreground": "#26C6DA",
          "template": "üòà ",
          "properties": {
            "macos": "mac"
          }
        },
        {
          "type": "path",
          "style": "plain",
          "foreground": "lightGreen",
          "template": "{{ .Path }} ",
          "properties": {
            "style": "folder"
          }
        },
        {
          "type": "git",
          "style": "plain",
          "template": "<#ffffff>on</> [{{ .HEAD }}{{ if gt .StashCount 0 }} \uf692 {{ .StashCount }}{{ end }}] ",
          "properties": {
            "branch_icon": "",
            "fetch_stash_count": true
          }
        },
        {
          "type": "dotnet",
          "style": "plain",
          "foreground": "#906cff",
          "template": "[\ue70c {{ .Full }}] "
        },
        {
          "type": "python",
          "style": "plain",
          "foreground": "#906cff",
          "template": "[\ue235 {{ if .Error }}{{ .Error }}{{ else }}{{ if .Venv }}{{ .Venv }} {{ end }}{{ .Full }}{{ end }}] "
        },
        {
          "type": "go",
          "style": "plain",
          "foreground": "#7FD5EA",
          "template": "[\ufcd1 {{ if .Error }}{{ .Error }}{{ else }}{{ .Full }}{{ end }}] "
        },
        {
          "type": "node",
          "style": "plain",
          "foreground": "#76b367",
          "template": "[\ue718 {{ if .PackageManagerIcon }}{{ .PackageManagerIcon }} {{ end }}{{ .Full }}] "
        },
        {
          "type": "ruby",
          "style": "plain",
          "foreground": "#f44336",
          "template": "[\ue791{{ if .Error }}{{ .Error }}{{ else }}{{ .Full }}{{ end }}] "
        },
        {
          "type": "java",
          "style": "plain",
          "foreground": "#ea2d2e",
          "template": "[\ue738 {{ if .Error }}{{ .Error }}{{ else }}{{ .Full }}{{ end }}] "
        },
        {
          "type": "julia",
          "style": "plain",
          "foreground": "#4063D8",
          "template": " \ue624 {{ if .Error }}{{ .Error }}{{ else }}{{ .Full }}{{ end }} "
        },
        {
          "type": "text",
          "style": "plain",
          "foreground": "#FFD54F",
          "template": "‚ùØ "
        }
      ]
    }
  ]
}
