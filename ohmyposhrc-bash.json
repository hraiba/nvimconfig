{
  "$schema": "https://raw.githubusercontent.com/JanDeDobbeleer/oh-my-posh/main/themes/schema.json",
  "palette": {
        "os": "#ACB0BE",
        "closer": "p:os",
        "pink": "#F5C2E7",
        "lavender": "#B4BEFE",
        "blue":  "#89B4FA"
  },
  "blocks": [
    {
      "type": "prompt",
      "alignment": "left",
      "segments": [
        {
          "properties": {
            "cache_duration": "none",
            "macos": "mac"
          },
          "template": "\uebc9 ",
          "foreground":"p:os",
          "type": "os",
          "style": "plain"
        },
        {
          "properties": {
            "cache_duration": "none",
            "style": "folder"
          },
          "template": "[{{ .Path }}] ",
          "foreground": "p:blue",
          "type": "path",
          "style": "plain"
        },
        {
          "properties": {
            "cache_duration": "none",
            "fetch_stash_count": true,
            "fetch_status": true,
            "fetch_upstream_icon": true,
            "untracked_modes": {
              "/Users/user/Projects/oh-my-posh/": "no"
            }
          },
          "template": "[{{ .HEAD }}{{if .BranchStatus }} {{ .BranchStatus }}{{ end }}{{ if .Working.Changed }} \uf044 {{ .Working.String }}{{ end }}{{ if and (.Working.Changed) (.Staging.Changed) }} |{{ end }}{{ if .Staging.Changed }} \uf046 {{ .Staging.String }}{{ end }}{{ if gt .StashCount 0 }} \uf692 {{ .StashCount }}{{ end }}] ",
          "foreground": "p:pink",
          "type": "git",
          "style": "powerline",
          "foreground_templates": [
            "{{ if or (.Working.Changed) (.Staging.Changed) }}#fa4204{{ end }}",
            "{{ if and (gt .Ahead 0) (gt .Behind 0) }}#FFCC80{{ end }}",
            "{{ if gt .Ahead 0 }}#B388FF{{ end }}",
            "{{ if gt .Behind 0 }}#B388FB{{ end }}",
            "{{ if gt or .UpstreamGone }}#c41906 {{ end }}"
          ]
        },
        {
          "properties": {
            "cache_duration": "none"
          },
          "template": "[\ue70c {{ .Full }}] ",
          "foreground": "#906cff",
          "type": "dotnet",
          "style": "plain"
        },
        {
          "properties": {
            "cache_duration": "none"
          },
          "template": "[\ue235 {{ if .Error }}{{ .Error }}{{ else }}{{ if .Venv }}{{ .Venv }} {{ end }}{{ .Full }}{{ end }}] ",
          "foreground": "#906cff",
          "type": "python",
          "style": "plain"
        },
        {
          "properties": {
            "cache_duration": "none"
          },
          "template": "[\ufcd1 {{ if .Error }}{{ .Error }}{{ else }}{{ .Full }}{{ end }}] ",
          "foreground": "#7FD5EA",
          "type": "go",
          "style": "plain"
        },
        {
          "properties": {
            "cache_duration": "none"
          },
          "template": "[\ue718 {{ if .PackageManagerIcon }}{{ .PackageManagerIcon }} {{ end }}{{ .Full }}] ",
          "foreground": "#76b367",
          "type": "node",
          "style": "plain"
        },
        {
          "properties": {
            "cache_duration": "none"
          },
          "template": "[\ue791{{ if .Error }}{{ .Error }}{{ else }}{{ .Full }}{{ end }}] ",
          "foreground": "#f44336",
          "type": "ruby",
          "style": "plain"
        },
        {
          "properties": {
            "cache_duration": "none"
          },
          "template": "[\ue738 {{ if .Error }}{{ .Error }}{{ else }}{{ .Full }}{{ end }}] ",
          "foreground": "#ea2d2e",
          "type": "java",
          "style": "plain"
        },
        {
          "properties": {
            "cache_duration": "none"
          },
          "template": " \ue624 {{ if .Error }}{{ .Error }}{{ else }}{{ .Full }}{{ end }} ",
          "foreground": "#4063D8",
          "type": "julia",
          "style": "plain"
        },
        {
          "properties": {
            "cache_duration": "none"
          },
          "template": "î¯† ",
          "foreground": "#FFD54F",
          "type": "text",
          "style": "plain"
        }
      ]
    }
  ],
  "final_space": true,
  "version": 3
}
